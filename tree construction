#include<bits/stdc++.h>
using namespace std;

int arr[]={9,1,3,4,5,6,7,8,0};


void constr(int l,int r,vector<int>&node)
{
    if(l>r)
    {
        return;
    }
        sort(arr, arr+9);
    int mid =(r+l)/2;
    node.push_back(arr[mid]);
    constr(l,mid-1,node);
    constr(mid+1,r,node);

}



int main()
{
    vector<int>node;
        constr(0,8,node);

   for(int i=0;i<9;i++)
        cout<<node[i]<<"  ";

}
